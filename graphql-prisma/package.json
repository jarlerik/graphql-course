  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^9.0.1",
    "graphql-cli": "2.16.4",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "1.16.7",
